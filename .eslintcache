t('YYYY-MM-DD HH:mm:ss');\n        // Output of dateString: 2020-07-21 07:24:06\n        console.log(img)\n\n        db.ref(`${user[0].username}_${otherUser}`)\n            .push({\n                message: text,\n                time: dateStringWithTime,\n                user: user[0].username,\n                image_url: ln,\n            })\n            .then(() => {\n                // do nothing for now\n            });\n\n        db.ref(`${otherUser}_${user[0].username}`)\n            .push({\n                message: text,\n                time: dateStringWithTime,\n                user: user[0].username,\n                image_url: img,\n            })\n            .then(() => {\n                // reset things\n                console.log(\"url here\")\n                console.log(ur)\n            });\n        setText(\"\");\n\n        dummy.current.scrollIntoView({ behavior: 'smooth' });\n    }\n\n    const handleChange = (e) => {\n        setText(e.target.value);\n    }\n\n\n    useEffect(() => {\n        if (otherUser && !isTest) {\n            db.ref(`${user[0].username}_${otherUser}`).on(\"value\", snapshot => {\n                let allMessages = [];\n                snapshot.forEach(snap => {\n                    allMessages.push(snap.val());\n                });\n                setMessages(allMessages);\n            });\n            scrollToBottom();\n        }\n    }, [otherUser]);\n\n    // useFocusEffect(\n    //     React.useCallback(() => {\n    //       const unsubscribe = API.subscribe(userId, user => setUser(data));\n\n    //       return () => unsubscribe();\n    //     }, [userId])\n    //   );\n\n    // scrollToBottom();\n\n    if (!otherUser) {\n        return (\n            <React.Fragment>\n                <NavBar user={user} setUser={setUser} />\n                <FeedLeft user={user} setUser={setUser} setOtherUser={setOtherUser} />\n                <div className=\"chat-window\">\n\n                    <h1 style={{ color: 'gray', margi